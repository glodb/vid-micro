{
    "database": {
        "dbname": "titles"
    },
    "microServiceName": "titlesService",
    "address": "localhost:8081",
    "controllers": ["Genres", "Status", "Language", "TitleType", "Titles"],
    "allowDB": true,
    "serviceLogName": "titles",
    "publishingTopics": [
        "vidmicro.language.created",
        "vidmicro.language.updated",
        "vidmicro.language.deleted",
        "vidmicro.title.created",
        "vidmicro.title.updated",
        "vidmicro.title.deleted",
        "vidmicro.title.language.deleted",
        "vidmicro.title.language.added"
    ],
    "subscribedTopics": {
    },
    "apis": {
        "/api/genres":["GET", "PUT", "POST", "DELETE"],
        "/api/status":["GET", "PUT", "POST", "DELETE"],
        "/api/languages":["GET", "PUT", "POST", "DELETE"],
        "/api/title_type":["GET", "PUT", "POST", "DELETE"],
        "/api/titles": ["GET", "PUT", "POST", "DELETE"],
        "/api/add_title_language":["POST"],
        "/api/delete_title_language":["POST"]
    },
    "sessionSecret": "spiderman",
    "acl": {
        "user": {
            "/api/genres":["GET"],
            "/api/status":["GET"],
            "/api/languages":["GET"],
            "/api/title_type":["GET"],
            "/api/titles": ["GET"]
        },
        "admin": {
            "/api/genres":["GET","PUT","POST","DELETE"],
            "/api/status":["GET", "PUT", "POST", "DELETE"],
            "/api/languages":["GET", "PUT", "POST", "DELETE"],
            "/api/title_type":["GET", "PUT", "POST", "DELETE"],
            "/api/titles": ["GET", "PUT", "POST", "DELETE"],
            "/api/add_title_language":["POST"],
            "/api/delete_title_language":["POST"]
        },
        "manager": {
            "/api/genres":["GET"],
            "/api/status":["GET"],
            "/api/title_type":["GET"],
            "/api/languages":["GET"],
            "/api/titles": ["GET"]
        }
    },
    "titlesPostfix" :"titles_cached_pages",
    "singleTitlePostfix": "single_",
    "languagePostfix": "_titles_languages",
    "statusPostfix": "_titles_status",
    "languageMetadataPostfix": "_titles_languages_metadata_postfix",
    "languageMetaExpiryTime":600000,
    "titleExpiryTime": 600000
}